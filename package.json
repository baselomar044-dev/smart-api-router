{
  "name": "try-it-ai",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Enterprise AI Assistant - Chat, Voice, Search & More",
  "author": "Basel",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/server.js --format=esm --external:better-sqlite3 --external:fs --external:path --external:crypto --external:dotenv",
    "preview": "vite preview",
    "start": "node dist/server.js",
    "start:server": "tsx server/index.ts",
    "start:local": "npm run build && npm run start",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.93.3",
    "@types/file-saver": "^2.0.7",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.5",
    "cors": "^2.8.6",
    "docx": "^9.5.1",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.0",
    "file-saver": "^2.0.5",
    "groq-sdk": "^0.8.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.468.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "openai": "^4.77.0",
    "puppeteer": "^24.36.1",
    "qrcode.react": "^4.2.0",
    "rate-limiter-flexible": "^9.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.1.0",
    "remark-gfm": "^4.0.0",
    "socket.io": "^4.8.3",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.15",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
